{
    "dashboard": {
        "id": null,
        "title": "NAIS {{cluster}} cluster dashboard",
        "tags": [
            "kubernetes",
            "nais"
        ],
        "timezone": "browser",
        "panels": [
            {
                "type": "text",
                "text": "{{cluster}}"
            },
            {
                "type": "singleStat",
                "title": "No of pods",
                "datasource": "{{cluster}}",
                "measurement": "kubePodContainerInfoCount"
            },
            {
                "type": "singleStat",
                "title": "Pending pods",
                "datasource": "{{cluster}}",
                "measurement": "kubePodStatusPhasePending"
            },
            {
                "type": "singleStat",
                "title": "Available deployments",
                "datasource": "{{cluster}}",
                "measurement": "kubeDeploymentsStatusReplicasAvailable"
            },
            {
                "type": "singleStat",
                "title": "Unavailable deployments",
                "datasource": "{{cluster}}",
                "measurement": "kubeDeploymentsStatusReplicasUnavailable"
            },
            {
                "type": "singleStat",
                "title": "Cores usage",
                "datasource": "{{cluster}}",
                "postfix": "cores",
                "measurement": "totalClusterCoresUsage"
            },
            {
                "type": "singleStat",
                "title": "Total CPU",
                "datasource": "{{cluster}}",
                "postfix": "cores",
                "measurement": "totalClusterCores"
            },
            {
                "type": "singleStat",
                "title": "Memory usage",
                "datasource": "{{cluster}}",
                "postfix": "GB",
                "measurement": "totalClusterMemoryUsage"
            },
            {
                "type": "singleStat",
                "title": "Total memory",
                "datasource": "{{cluster}}",
                "postfix": "GB",
                "measurement": "totalClusterMemory"
            },
            {
                "type": "singleStat",
                "title": "Storage usage",
                "datasource": "{{cluster}}",
                "postfix": "GB",
                "measurement": "totalClusterStorageUsage"
            },
            {
                "type": "singleStat",
                "title": "Total storage",
                "datasource": "{{cluster}}",
                "postfix": "?",
                "measurement": "totalClusterStorage"
            },
            {
                "type": "singleStatGauge",
                "title": "CPU usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterCoresUsagePercent"
            },
            {
                "type": "singleStatGauge",
                "title": "Memory usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterMemoryUsagePercent"
            },
            {
                "type": "singleStatGauge",
                "title": "Storage usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterStorageUsagePercent"
            },
            {
                "type": "singleStatGauge",
                "title": "Allocated cores",
                "datasource": "{{cluster}}",
                "postfix": "%",
                "measurement": "totalClusterCoresAllocatedPercent"
            },
            {
                "type": "singleStatGauge",
                "title": "Allocated memory",
                "datasource": "{{cluster}}",
                "postfix": "%",
                "measurement": "totalClusterMemoryAllocatedPercent"
            },
            {
                "type": "statusPanel",
                "nodes": "{{masters}}",
                "title": "Master status",
                "measurement": "aggregateStatusPanel"
            },
            {
                "type": "statusPanel",
                "nodes": "{{workers}}",
                "title": "Worker status",
                "measurement": "aggregateStatusPanel"
            },
            {
                "type": "row",
                "repeating": true,
                "collapsed": false,
                "nodes": "{{masters}}",
                "panels": [
                    {
                        "type": "discrete",
                        "nodes": null,
                        "timeFrom": "24h",
                        "title": "Master component status",
                        "measurement": [
                            "processDockerd",
                            "processKubelet",
                            "interfaceDocker0",
                            "interfaceFlannel",
                            "componentControllerManager",
                            "componentScheduler",
                            "componentEtcd0",
                            "addonCoredns",
                            "addonKubernetesDashboard",
                            "addonTillerDeploy"
                        ]
                    },
                    {
                        "type": "graph",
                        "measurement": "cpuIdle",
                        "title": "Node CPU Load"
                    },
                    {
                        "type": "graph",
                        "stack": true,
                        "measurement": [
                            "memoryUsageWOBuffersCaches",
                            "memoryPercentCached"
                        ],
                        "title": "Node Memory Usage"
                    },
                    {
                        "type": "graph",
                        "measurement": "diskVarUsage",
                        "title": "Node Disk Usage (/var)"
                    }
                ]
            },
            {
                "type": "row",
                "repeating": true,
                "collapsed": false,
                "nodes": "{{workers}}",
                "panels": [
                    {
                        "type": "discrete",
                        "nodes": null,
                        "timeFrom": "24h",
                        "title": "Worker component status",
                        "measurement": [
                            "processDockerd",
                            "processKubelet",
                            "processFlanneld",
                            "processKubeProxy",
                            "interfaceDocker0",
                            "interfaceFlannel"
                        ]
                    },
                    {
                        "type": "graph",
                        "measurement": "cpuIdle",
                        "title": "Node CPU Load"
                    },
                    {
                        "type": "graph",
                        "stack": true,
                        "measurement": [
                            "memoryUsageWOBuffersCaches",
                            "memoryPercentCached"
                        ],
                        "title": "Node Memory Usage"
                    },
                    {
                        "type": "graph",
                        "measurement": "diskVarUsage",
                        "title": "Node Disk Usage (/var)"
                    }
                ]
            }
        ],
        "schemaVersion": 6,
        "version": 0
    },
    "overwrite": true
}