{
    "dashboard": {
        "id": null,
        "title": "NAIS {{cluster}} cluster dashboard",
        "tags": [
            "kubernetes",
            "nais"
        ],
        "timezone": "browser",
        "panels": [
            {
                "type": "text",
                "text": "{{cluster}}"
            },
            {
                "type": "singleStat",
                "title": "No of pods",
                "datasource": "{{cluster}}",
                "measurement": "kubePodContainerInfoCount"
            },
            {
                "type": "singleStat",
                "title": "Pending pods",
                "datasource": "{{cluster}}",
                "measurement": "kubePodStatusPhasePending"
            },
            {
                "type": "singleStat",
                "title": "Available deployments",
                "datasource": "{{cluster}}",
                "measurement": "kubeDeploymentsStatusReplicasAvailable"
            },
            {
                "type": "singleStat",
                "title": "Unavailable deployments",
                "datasource": "{{cluster}}",
                "measurement": "kubeDeploymentsStatusReplicasUnavailable"
            },
            {
                "type": "statusPanel",
                "nodes": "{{masters}}",
                "title": "Master status",
                "measurement": "aggregateStatusPanel",
                "gridPos": {
                    "h": 3,
                    "w": 12
                }
            },
            {
                "type": "statusPanel",
                "nodes": "{{workers}}",
                "title": "Worker status",
                "measurement": "aggregateStatusPanel",
                "gridPos": {
                    "h": 3,
                    "w": 12
                }
            },
            {
                "type": "singleStatGauge",
                "title": "CPU usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterCoresUsagePercent"
            },
            {
                "type": "singleStatGauge",
                "title": "Memory usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterMemoryUsagePercent"
            },
            {
                "type": "singleStatGauge",
                "title": "Storage usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterStorageUsagePercent"
            },
            {
                "type": "singleStatGauge",
                "title": "Allocated cores",
                "datasource": "{{cluster}}",
                "postfix": "%",
                "measurement": "totalClusterCoresAllocatedPercent"
            },
            {
                "type": "singleStatGauge",
                "title": "Allocated memory",
                "datasource": "{{cluster}}",
                "postfix": "%",
                "measurement": "totalClusterMemoryAllocatedPercent"
            },
            {
                "type": "singleStat",
                "title": "Successful requests last 5 minutes",
                "datasource": "{{cluster}}",
                "sparkline": true,
                "measurement": "successfulRequests",
                "gridPos": {
                    "h": 6,
                    "w": 4
                }
            },
            {
                "type": "singleStat",
                "title": "Failed requests last 5 minutes",
                "datasource": "{{cluster}}",
                "sparkline": true,
                "measurement": "failedRequests",
                "gridPos": {
                    "h": 6,
                    "w": 4
                }            
            },
            {
                "type": "graph",
                "datasource": "{{cluster}}",
                "format": "none",
                "measurement": [
                    "successfulRequestsCodes"
                ],
                "title": "Successful status code count (5m)"
            },
            {
                "type": "graph",
                "datasource": "{{cluster}}",
                "format": "none",
                "measurement": [
                    "failedRequestsCodes"
                ],
                "title": "Failed status code count (5m)"
            },
            {
                "type": "responseHistogramGraph",
                "datasource": "{{cluster}}",
                "gridPos": {
                    "h": 8,
                    "w": 8
                }            
            },
            {
                "type": "row",
                "repeating": true,
                "collapsed": false,
                "nodes": "{{masters}}",
                "panels": [
                    {
                        "type": "discrete",
                        "timeFrom": "24h",
                        "title": "Master component status",
                        "measurement": [
                            "processDockerd",
                            "processKubelet",
                            "interfaceDocker0",
                            "interfaceFlannel",
                            "componentControllerManager",
                            "componentScheduler",
                            "componentEtcd0",
                            "addonCoredns",
                            "addonKubernetesDashboard",
                            "addonTillerDeploy"
                        ]
                    },
                    {
                        "type": "graph",
                        "measurement": "cpuIdle",
                        "max": 100,
                        "title": "Node CPU Load"
                    },
                    {
                        "type": "graph",
                        "stack": true,
                        "max": 100,
                        "measurement": [
                            "memoryUsageWOBuffersCaches",
                            "memoryPercentCached"
                        ],
                        "title": "Node Memory Usage"
                    },
                    {
                        "type": "graph",
                        "max": 100,
                        "measurement": "diskVarUsage",
                        "title": "Node Disk Usage (/var)"
                    }
                ]
            },
            {
                "type": "row",
                "repeating": true,
                "collapsed": false,
                "nodes": "{{workers}}",
                "panels": [
                    {
                        "type": "discrete",
                        "nodes": null,
                        "timeFrom": "24h",
                        "title": "Worker component status",
                        "measurement": [
                            "processDockerd",
                            "processKubelet",
                            "processFlanneld",
                            "processKubeProxy",
                            "interfaceDocker0",
                            "interfaceFlannel"
                        ]
                    },
                    {
                        "type": "graph",
                        "measurement": "cpuIdle",
                        "max": 100,
                        "title": "Node CPU Load"
                    },
                    {
                        "type": "graph",
                        "stack": true,
                        "max": 100,
                        "measurement": [
                            "memoryUsageWOBuffersCaches",
                            "memoryPercentCached"
                        ],
                        "title": "Node Memory Usage"
                    },
                    {
                        "type": "graph",
                        "max": 100,
                        "measurement": "diskVarUsage",
                        "title": "Node Disk Usage (/var)"
                    }
                ]
            }
        ],
        "schemaVersion": 6,
        "version": 0
    },
    "overwrite": true
}