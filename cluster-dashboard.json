{
    "dashboard": {
        "id": null,
        "title": "NAIS {{cluster}} cluster dashboard",
        "tags": [
            "kubernetes",
            "nais"
        ],
        "timezone": "browser",
        "panels": [
            {
                "panel": "text",
                "text": "{{cluster}}",
                "gridPos": {
                    "h": 3,
                    "w": 8,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "panel": "singleStat",
                "title": "No of pods",
                "datasource": "{{cluster}}",
                "measurement": "kubePodContainerInfoCount",
                "gridPos": {
                    "h": 3,
                    "w": 4,
                    "x": 8,
                    "y": 0
                }
            },
            {
                "panel": "singleStat",
                "title": "Pending pods",
                "datasource": "{{cluster}}",
                "measurement": "kubePodStatusPhasePending",
                "gridPos": {
                    "h": 3,
                    "w": 4,
                    "x": 12,
                    "y": 0
                }
            },
            {
                "panel": "singleStat",
                "title": "Available deployments",
                "datasource": "{{cluster}}",
                "measurement": "kubeDeploymentsStatusReplicasAvailable",
                "gridPos": {
                    "h": 3,
                    "w": 4,
                    "x": 16,
                    "y": 0
                }
            },
            {
                "panel": "singleStat",
                "title": "Unavailable deployments",
                "datasource": "{{cluster}}",
                "measurement": "kubeDeploymentsStatusReplicasUnavailable",
                "gridPos": {
                    "h": 3,
                    "w": 4,
                    "x": 20,
                    "y": 0
                }
            },
            {
                "panel": "singleStat",
                "title": "Cores usage",
                "datasource": "{{cluster}}",
                "postfix": "cores",
                "measurement": "totalClusterCoresUsage",
                "gridPos": {
                    "h": 3,
                    "w": 4,
                    "x": 0,
                    "y": 3
                }
            },
            {
                "panel": "singleStat",
                "title": "Total CPU",
                "datasource": "{{cluster}}",
                "postfix": "cores",
                "measurement": "totalClusterCores",
                "gridPos": {
                    "h": 3,
                    "w": 4,
                    "x": 4,
                    "y": 3
                }
            },
            {
                "panel": "singleStat",
                "title": "Memory usage",
                "datasource": "{{cluster}}",
                "postfix": "GB",
                "measurement": "totalClusterMemoryUsage",
                "gridPos": {
                    "h": 3,
                    "w": 4,
                    "x": 8,
                    "y": 3
                }
            },
            {
                "panel": "singleStat",
                "title": "Total memory",
                "datasource": "{{cluster}}",
                "postfix": "GB",
                "measurement": "totalClusterMemory",
                "gridPos": {
                    "h": 3,
                    "w": 4,
                    "x": 12,
                    "y": 3
                }
            },
            {
                "panel": "singleStat",
                "title": "Storage usage",
                "datasource": "{{cluster}}",
                "postfix": "GB",
                "measurement": "totalClusterStorageUsage",
                "gridPos": {
                    "h": 3,
                    "w": 4,
                    "x": 16,
                    "y": 3
                }
            },
            {
                "panel": "singleStat",
                "title": "Total storage",
                "datasource": "{{cluster}}",
                "postfix": "?",
                "measurement": "totalClusterStorage",
                "gridPos": {
                    "h": 3,
                    "w": 4,
                    "x": 20,
                    "y": 3
                }
            },
            {
                "panel": "singleStat",
                "subtype": "gauge",
                "title": "CPU usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterCoresUsagePercent",
                "gridPos": {
                    "h": 6,
                    "w": 8,
                    "x": 0,
                    "y": 6
                }
            },
            {
                "panel": "singleStat",
                "subtype": "gauge",
                "title": "Memory usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterMemoryUsagePercent",
                "gridPos": {
                    "h": 6,
                    "w": 8,
                    "x": 8,
                    "y": 6
                }
            },
            {
                "panel": "singleStat",
                "subtype": "gauge",
                "title": "Storage usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterStorageUsagePercent",
                "gridPos": {
                    "h": 6,
                    "w": 8,
                    "x": 16,
                    "y": 6
                }
            },
       
            {
                "panel": "singleStat",
                "subtype": "gauge",
                "title": "Allocated cores",
                "datasource": "{{cluster}}",
                "postfix": "%",
                "measurement": "totalClusterCoresAllocatedPercent",
                "gridPos": {
                    "h": 6,
                    "w": 8,
                    "x": 0,
                    "y": 12
                }
            },
            {
                "panel": "singleStat",
                "subtype": "gauge",
                "title": "Allocated memory",
                "datasource": "{{cluster}}",
                "postfix": "%",
                "measurement": "totalClusterMemoryAllocatedPercent",
                "gridPos": {
                    "h": 6,
                    "w": 8,
                    "x": 8,
                    "y": 12
                }
            },
            {
                "panel": "statusPanel",
                "nodes": "{{workers}}",
                "title": "Worker status",
                "measurement": "aggregateStatusPanel",
                "gridPos": {
                    "h": 3,
                    "w": 12,
                    "x": 0,
                    "y": 27
                }
            },
            {
                "panel": "statusPanel",
                "nodes": "{{masters}}",
                "title": "Master status",
                "measurement": "aggregateStatusPanel",
                "gridPos": {
                    "h": 3,
                    "w": 12,
                    "x": 12,
                    "y": 27
                }
            }
        ],
        "schemaVersion": 6,
        "version": 0
    },
    "overwrite": true
}