{
    "dashboard": {
        "id": null,
        "title": "NAIS {{cluster}} single page cluster dashboard",
        "folderId": 555,
        "tags": [
            "kubernetes",
            "nais"
        ],
        "timezone": "browser",
        "panels": [
            {
                "type": "text",
                "text": "{{cluster}}"
            },
            {
                "type": "singleStat",
                "title": "No of pods",
                "datasource": "{{cluster}}",
                "measurement": "kubePodContainerInfoCount"
            },
            {
                "type": "singleStat",
                "title": "Pending pods",
                "datasource": "{{cluster}}",
                "measurement": "kubePodStatusPhasePending"
            },
            {
                "type": "singleStat",
                "title": "Available deployments",
                "datasource": "{{cluster}}",
                "measurement": "kubeDeploymentsStatusReplicasAvailable"
            },
            {
                "type": "singleStat",
                "title": "Unavailable deployments",
                "datasource": "{{cluster}}",
                "measurement": "kubeDeploymentsStatusReplicasUnavailable"
            },
            {
                "type": "statusPanel",
                "nodes": "{{masters}}",
                "title": "Master status",
                "measurement": "aggregateStatusPanel",
                "gridPos": {
                    "h": 3,
                    "w": 12
                }
            },
            {
                "type": "statusPanel",
                "nodes": "{{workers}}",
                "title": "Worker status",
                "measurement": "aggregateStatusPanel",
                "gridPos": {
                    "h": 3,
                    "w": 12
                }
            },
            {
                "type": "singleStatGauge",
                "title": "CPU usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterCoresUsagePercent",
                "gridPos": {
                    "h": 5,
                    "w": 4
                }
            },
            {
                "type": "singleStatGauge",
                "title": "Memory usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterMemoryUsagePercent",
                "gridPos": {
                    "h": 5,
                    "w": 4
                }
            },
            {
                "type": "singleStatGauge",
                "title": "Storage usage",
                "postfix": "%",
                "datasource": "{{cluster}}",
                "measurement": "totalClusterStorageUsagePercent",
                "gridPos": {
                    "h": 5,
                    "w": 4
                }
            },
            {
                "type": "singleStatGauge",
                "title": "Allocated cores",
                "datasource": "{{cluster}}",
                "postfix": "%",
                "measurement": "totalClusterCoresAllocatedPercent",
                "gridPos": {
                    "h": 5,
                    "w": 4
                }
            },
            {
                "type": "singleStatGauge",
                "title": "Allocated memory",
                "datasource": "{{cluster}}",
                "postfix": "%",
                "measurement": "totalClusterMemoryAllocatedPercent",
                "gridPos": {
                    "h": 5,
                    "w": 4
                }
            },
            {
                "type": "singleStat",
                "title": "Successful requests last 5m",
                "datasource": "{{cluster}}",
                "sparkline": true,
                "measurement": "successfulRequests",
                "gridPos": {
                    "h": 5,
                    "w": 2
                }
            },
            {
                "type": "singleStat",
                "title": "Failed requests last 5m",
                "datasource": "{{cluster}}",
                "sparkline": true,
                "measurement": "failedRequests",
                "gridPos": {
                    "h": 5,
                    "w": 2                
                }
            },
            {
                "type": "graph",
                "datasource": "{{cluster}}",
                "format": "none",
                "measurement": [
                    "successfulRequestsCodes"
                ],
                "title": "Successful status code count (5m)"
            },
            {
                "type": "graph",
                "datasource": "{{cluster}}",
                "format": "none",
                "measurement": [
                    "failedRequestsCodes"
                ],
                "title": "Failed status code count (5m)"
            },
            {
                "type": "responseHistogramGraph",
                "datasource": "{{cluster}}"
            }
        ],
        "schemaVersion": 6,
        "version": 0
    },
    "overwrite": true
}